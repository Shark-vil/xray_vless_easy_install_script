{
    "outbounds": [
      {
        "type": "vless", 
        "tag": "vless-out",
        "server": "%DOMAIN_NAME%",
        "server_port": 443,
        "uuid": "%CLIENT_UUID%",
        "flow": null,
        "tls": {
          "enabled": true,
          "server_name": "%DOMAIN_NAME%",
          "insecure": true,
          "utls": {
            "enabled": true,
            "fingerprint": "chrome"
          }
        },
        "multiplex": {
          "enabled": false,
          "protocol": "smux",
          "max_streams": 32
        },
        "packet_encoding": "xudp",
        "transport": {
          "type": "ws",
          "path": "/%WEBSOCKET_PATH%",
          "headers": {
            "Host": "%DOMAIN_NAME%"
          }
        }
      }
    ]
  }
  